<div class="destination-ticket">
    <div class="destination-left">
        <img src="{{imgUrl}}" alt="">
    </div>
    <div class="destination-right">
        <div>
            <h3>{{name}}</h3><span>{{departure}}</span>
        </div>
        <div>
            to {{city}}
        </div>
        <p>{{duration}} days </p>
        <a href="#/delete/{{objectId}}" class="remove">REMOVE</a>
        <a href="#/details/{{objectId}}" class="details">Details</a>
    </div>

</div>